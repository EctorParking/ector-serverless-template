{
  "env": {
    "node": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "import/no-named-as-default-member": [0],
    "no-underscore-dangle": [0],
    "no-console": [0],
    "prettier/prettier": ["error", { "singleQuote": true, "trailingComma": "es5"}]
  },
  "plugins": [
    "prettier"
  ]
}
